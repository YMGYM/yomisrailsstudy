<h1>Test#index</h1>
<p>Find me in app/views/test/index.html.erb</p>
<table>
  <tr>
    <th>No.</th>
    <th>Title</th>
    <th>Content</th>
    <th></th>
  </tr>

  <% @test.each do |test| %>
  <tr>
    <td><%= test.id %></td>
    <td><%= test.title %></td>
    <td><%= test.content %></td>
    <td><%= link_to "글 보기", test_path(test) %></td>
  </tr>
  <% end %>

</table>
<%= link_to "새 글 쓰기", new_test_path %>